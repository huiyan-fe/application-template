{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": ">1%",
                "loose": true,
                "bugfixes": true
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}
